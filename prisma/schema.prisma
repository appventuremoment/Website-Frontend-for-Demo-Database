generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model accounts {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(50)
  password String @db.VarChar(250)
  email    String @unique(map: "email") @db.VarChar(100)
}
